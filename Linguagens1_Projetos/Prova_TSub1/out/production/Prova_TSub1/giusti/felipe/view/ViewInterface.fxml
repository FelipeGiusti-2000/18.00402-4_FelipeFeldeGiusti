                        <Insets />
                     </VBox.margin>
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="imgCard" fitHeight="327.0" fitWidth="299.0" pickOnBounds="true" preserveRatio="true" />
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="64.0">
                     <children>
                        <Button fx:id="btnLastCard" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="200.0" text="&lt;-" />
