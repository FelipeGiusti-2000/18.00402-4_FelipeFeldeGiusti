<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="giusti.felipe.controller.ControllerInterface">
   <children>
      <HBox layoutX="-1.0" prefHeight="410.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" prefHeight="400.0" prefWidth="170.0" spacing="8.0">
               <children>
                  <TextField fx:id="txtId_Reg" maxWidth="1.7976931348623157E308" promptText="id:">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="txtUrl_Reg" maxWidth="1.7976931348623157E308" promptText="URL Imagem:">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="txtName_Reg" maxWidth="1.7976931348623157E308" promptText="nome:">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="txtRarity_Reg" maxWidth="1.7976931348623157E308" promptText="raridade:">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="txtSeries_Reg" maxWidth="1.7976931348623157E308" promptText="série:">
                     <VBox.margin>
                        <Insets />
