                  <TextField fx:id="txtName_Card" promptText="nome:" />
                  <TextField fx:id="txtRarity_Card" promptText="raridade:" />
                  <TextField fx:id="txtSeries_Card" promptText="série:" />
                  <TextField fx:id="txtCardSet_Card" promptText="coleção:" />
